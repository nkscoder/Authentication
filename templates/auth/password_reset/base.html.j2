  {% block header %}

  <!-- ======== full nav menu ====== -->
  <div id="myNav" class="workly-custom overlay">
    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>

    <div class="overlay-content">
      <div class="row">

        {% for cate in location_category(request) %}
        {% if not cate.parent %}
        <div class="col-sm-4 col-xs-12">
          <p class="h3">{{cate}}</p>
          <ul>
            {% for chid in location_category(request) %}
            {% if cate == chid.parent %}
            <li><a href='/location/{{ chid.slug }}'>{{ chid.name }}</a></li>
            {% endif %}
            {% endfor %}
          </ul>

        </div>

        {% endif %}
        {% endfor %}
      </div>
    </div>

  </div>
  <!-- ======== full nav menu end ====== -->

