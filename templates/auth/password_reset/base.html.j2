      event.preventDefault();
      $.ajaxSettings.traditional = true;
      console.log(orderForm);
      $.ajax({
        type: 'POST',
        url: '/signup/',
        dataType: 'json',
        data: $(orderForm).serializeArray(),
        success: function (response) {
          $("#signup_spinner").css("display", "none");
          $('#errorConPassword').empty();
          $('#errorPassword').empty();
          $('#errorPhone').empty();
          $('#errorEmail').empty();
          $('#errorName').empty();
          $('#errorPassword').empty();
          $('#success').empty();
          $('#some').empty();

          if (response.confirm_password) {
            $('#errorConPassword').text(response.confirm_password);
          }
          if (response.password) {
            $('#errorPassword').text(response.password);
          }
          if (response.mobile) {
            $('#errorPhone').text(response.mobile);
          }
          if (response.email) {
            $('#errorEmail').text(response.email);
          }
          if (response.name) {
            $('#errorName').text(response.name);
          }
          if (response.password_l) {
            $('#errorPassword').text(response.password_l);
          }
          if (response.success) {
            $('#success').text(response.success);
          }
          if (response.some) {
            $('#some').text(response.some);
          }


        }
      });
    })



    $(document).on('submit', '#login', function (event) {
      var orderForm = document.getElementById("login");
      $('#success_login').empty();
      $('#error_login').empty();

      $("#login_spinner").css("display", "inline-block");
      event.preventDefault();
      $.ajaxSettings.traditional = true;
      console.log(orderForm);
      $.ajax({
        type: 'POST',
        url: '/login/',
        dataType: 'json',
        data: $(orderForm).serializeArray(),
        success: function (response) {
          // username
          $('#success_login').empty();
          $('#error_login').empty();
          $("#login_spinner").css("display", "none");
          if (response.success) {
            $('#success_login').text(response.success);
            setTimeout(function () { window.location.reload(1); }, 3000);
          }
          if (response.error) {
            $('#error_login').text(response.error);
          }


        }
      });
    })



    $(document).on('submit', '#forgot', function (event) {
      var orderForm = document.getElementById("forgot");
      event.preventDefault();
      $.ajaxSettings.traditional = true;
      console.log(orderForm);
      $.ajax({
        type: 'POST',
        url: '/reset/password/sent/',
        dataType: 'json',
        data: $(orderForm).serializeArray(),
        success: function (response) {
          $('#success_forgot').empty();
          $('#error_forgot').empty();
          if (response.success) {

            $('#success_forgot').text(response.success);
            setTimeout(function () { window.location.reload(1); }, 3000);
          }
          if (response.error) {
            $('#error_forgot').text(response.error);
          }


        }
      });
    })

  </script>


